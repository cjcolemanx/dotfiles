! Black color that will not be affected by bold highlighting.
*.color66: #010f21
*color66:  #010f21

! Xclock colors.
XClock*foreground: #bfc3c7
XClock*background: #010f21
XClock*majorColor:  rgba:bf/c3/c7/ff
XClock*minorColor:  rgba:bf/c3/c7/ff
XClock*hourColor:   rgba:bf/c3/c7/ff
XClock*minuteColor: rgba:bf/c3/c7/ff
XClock*secondColor: rgba:bf/c3/c7/ff

! Set depth to make transparency work.
URxvt*depth: 32
